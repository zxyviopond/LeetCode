\f0\fs24 \cf0 public class Solution \{\
    public boolean isValid(String ss) \{\
        Stack<Character> s = new Stack<Character>();\
        char[] c = ss.toCharArray();\
        for(int i=0;i<c.length;i++)\{\
            if(c[i]=='('||c[i]=='['||c[i]=='\{')\{\
                s.push(c[i]);\
            \}\
            else\{\
                if(s.isEmpty()) return false;\
                Character top = s.peek();\
                Character thischar=c[i];\
                if((top=='['&&thischar==']')||(top=='('&&thischar==')')||(top=='\{'&&thischar=='\}'))\{\
                  s.pop();  \
                \}\
                else return false;\
            \}\
        \}\
        \
        if(s.isEmpty())\{\
        return true;\
        \}\
        return false;\
        \
    \}\
\}}